    "def find_fake_t(t):\n",
    "    N = len(t)\n",
    "    Tspan = t.max()-t.min()\n",
    "    dt = Tspan/(N-1)\n",
    "    nvect = np.arange(N)\n",
    "    fake_t = dt*nvect\n",
    "    return fake_t"
